{
  "name": "nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --verbose --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2": "pm2 restart ecosystem.config.js",
    "db": "rethinkdb",
    "redis": "redis-server",
    "rsync": "rsync -v -r dist package.json ecosystem.config.js root@128.199.216.159:/root/minichat",
    "deploy": "yarn build && yarn rsync"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.8",
    "@nestjs/core": "^7.0.8",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/platform-socket.io": "^7.0.8",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/websockets": "^7.0.8",
    "class-validator": "^0.12.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rethinkdb": "^2.4.2",
    "rethinkdb-ts": "^2.4.2",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.5",
    "socket.io-redis": "^5.2.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.8",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "25.4.0",
    "jest-extended": "^0.11.5",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  }
}
